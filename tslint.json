{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "react-hooks-nesting": "error",
    "import-name": [
      false
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ]
  },
  "rulesDirectory": []
}
